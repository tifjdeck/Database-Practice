        xmlns:local="clr-namespace:Database_Practice"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="MainTextBox" HorizontalAlignment="Left" Margin="32,29,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="311" Height="188"/>
